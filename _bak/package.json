{
  "name": "webpack-jekyll",
  "version": "1.0.0",
  "description": "Base structure for Jekyll built with webpack",
  "main": "src/index.js",
  "author": "Jason Taylor <jason@redant.com.au>",
  "private": true,
  "scripts": {
    "start": "rimraf ./assets && concurrently 'yarn start:webpack' 'yarn start:jekyll'",
    "start:webpack": "webpack-dev-server --host 0.0.0.0 --disable-host-check",
    "start:jekyll": "bundle exec jekyll build --watch",
    "build": "rimraf ./assets && yarn build:webpack && yarn build:jekyll",
    "build:webpack": "NODE_ENV=production webpack -p --bail --progress --profile",
    "build:jekyll": "JEKYLL_ENV=production bundle exec jekyll build"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "aos": "^2.2.0",
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "bootstrap": "4.0.0-beta.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "parallax-js": "^3.1.0",
    "popper.js": "^1.12.6",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "slick-carousel": "^1.8.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-manifest-plugin": "^1.3.2"
  }
}
